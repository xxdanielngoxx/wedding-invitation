generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Todo {
  id        Int      @id @default(autoincrement())
  title     String
  createdAt DateTime @default(now())
}

model Registration {
  id         Int      @id @default(autoincrement())
  name       String
  attendance Boolean
  message    String
  createdAt  DateTime @default(now())
}
